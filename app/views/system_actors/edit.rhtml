<%= title(_('Editing %s') % SystemActor.describe(@actor)) %>

<% form_tag :action => 'update', :id => @system_actor do %>
  <%= render :partial => 'form' %>
  <div class='button_bar'>
    <%= button _('Save'), :save %>
    <%= button _('Cancel'), :cancel, :action => 'list', :actor => @actor %>
  </div>
<% end %>
