<h2><%= _('Users') %></h2>

<table>
  <tr>
    <th><%= _('Login') %></th>
  </tr>
  <% for user in @users %>
    <tr>
      <td><%= user.login %></td>
      <td>
        <%= button _('Edit'), :edit, :action => 'edit', :id => user %>
      </td>
    </tr>
  <% end%>
</table>

<div class='button_bar'>
<%= button _('New user'), :new, :action => 'new' %>
</div>
