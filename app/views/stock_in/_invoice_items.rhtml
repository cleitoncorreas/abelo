<table>
  <tr>
    <th> <%= _('Product') %> </th>
    <th> <%= _('Unit Price') %> </th>
    <th> <%= _('Amount') %> </th>
    <th> <%= _('Validity') %> </th>
  </tr>
  <% @stocks.each do |stock| %>
    <tr>
      <td> <%= stock.product.name %> </td>
      <td> <%= stock.price %> </td>
      <td> <%= stock.amount %> </td>
      <td> <%= stock.validity || _('None') %> </td>
    </tr>
  <% end %>

</table>

