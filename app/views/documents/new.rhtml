<%= title('New Document Model') %>

<%= error_messages_for "document" %>


<% form_tag :action => 'create' do %>
  
  <label><%= _('Name') %></label>
  <%= text_field 'document', 'name' %>
    
  <%= multiple_select('document', 'departments', @departments, _('Departments')) %>
  <%= button 'back',_('Back'), :back,  :action => 'list', :document_model => params[:document_model] %>
  <%= button 'save', _('Edit'), :save%>
<% end %>
