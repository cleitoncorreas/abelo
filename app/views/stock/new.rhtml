<%= title _('New stock entry for %s') % @product.name %>
<%= error_messages_for :entry %>
<% form_tag :action => 'add_entry', :id => @product do %>
<%= render :partial => 'form'%>
<div class='button_bar'>
<%= button 'save', '', :save %>
<%= button 'cancel',  '', :cancel, :action => 'history', :id => @product %>
</div>

<% end %>
