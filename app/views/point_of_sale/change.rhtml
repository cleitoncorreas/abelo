
<%= title _('Change') %>

<div class="change_sale">
  <p><%= _("Total: %s") % @total %> </p>
  <p><%= _("Total Payment: %s") % @total_payment %></p>
  <p><%= _("Change: %s") % (@total_payment - @total) %></p>
  <p><%= link_to '<span>'+_('End Sale')+'</span>', :action => 'create_close_sale', :sale_id => @sale %> </p>
</div>

