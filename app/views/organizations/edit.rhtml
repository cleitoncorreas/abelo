<%= title _('Editing Organization %s') % @organization.name %>

<% form_tag :action => 'update', :id => @organization  do %>
  <%= render :partial => 'form' %>
  <div class='button_bar'>
    <%= button 'save', _('Save'), :save %>
    <%= button 'back', _('Back'), :back, :action => 'list' %>
  </div>
<% end %>

