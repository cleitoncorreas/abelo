
<%= title @organization.name %>

<p> <b><%= _('Name') %>:</b> <%= @organization.name %></p>
<p> <b><%= _('CNPJ') %>:</b> <%= @organization.cnpj %></p>
<p> <b><%= _('Nickname') %>:</b> <%= @organization.identifier %></p>

<div class="display_info_options">
  <%= button('back', _('Back'), :list, :action => 'list' ) %>
  <%= button('edit', _('Edit'), :list, :action => 'edit', :organization_id => @organization) %> 
</div>
