<h2><%= _('Editing profile for %s') % @user_profile.user.login %></h2>

<%= error_messages_for 'user_profile' %>

<%= form_tag(:action => 'update_template', :id => @user_profile) %>
  <%= render :partial => 'form' %>
  <div class='button_bar'>
    <%= button _('Save') ,:save %>
    <%= button _('Cancel'), :cancel, :action => 'index' %>
  </div>
<%= end_form_tag %>
