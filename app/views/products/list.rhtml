
<%= title _('Listing products') %>

<% @products.map do |p| %>
  <%=
  display_list([
        [p.images.map{|i| show_image i}[0]], 
        [_('Code:'),p.id],
        [_('Description:'),p.description],
        [_('Price'), p.sell_price]
      ],
      [1,2,1],
      {:url_options => 'leo', :li_options => 'eldi'}
   )
  %>
<% end %>
